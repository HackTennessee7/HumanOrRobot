.containter-fluid
  #game-view
    .row
      .navbar.navbar-default.navbar-fixed-top
        .container
          h2.navbar-right score: 10pts
    .row
      h1 Human or Robot?
    .row
      .col-md-4.col-md-offset-4
        img.img-responsive.img-circle(src='assets/placeholder.jpg')
    .row
      button.btn.btn-large.btn-success(type='button' data-toggle='modal' data-target='#answer-modal') Human
      button.btn.btn-large.btn-warning(type='button' data-toggle='modal' data-target='#answer-modal') Robot
      
  
  include game-modals
